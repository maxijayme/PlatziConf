{
    "extends": [
      "prettier",
      "airbnb"
    ],
    "pluggins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "arrowParens": "avoid",
          "bracketSpacing": true,
          "htmlWhitespaceSensitivity": "css",
          "insertPragma": false,
          "jsxBracketSameLine": false,
          "jsxSingleQuote": true,
          "printWidth": 80,
          "proseWrap": "preserve",
          "quoteProps": "as-needed",
          "requirePragma": false,
          "semi": false,
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "none",
          "useTabs": false,
          "vueIndentScriptAndStyle": false
        }
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
  }
}